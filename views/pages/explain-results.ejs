<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head'); %>
    <link rel="stylesheet" href="/css/explain-results.css" />
    <script src="/js/explain-results.js" defer></script>
</head>

<body>
  <header><%- include('../partials/navbar'); %></header>
  <main>
    <% sections.forEach(function(section) { %>
      <div data-content="<%= section.content %>" data-summary="<%= section.summary %>" onmouseenter="showSummary(event)"
        onmouseout="hideSummary(event)">
        <%= section.content %>
      </div>
      <% }); %>
  </main>
</body>
